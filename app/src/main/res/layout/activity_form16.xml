<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".form.form16">

    <SeekBar
        android:id="@+id/form16_seekBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:max="10"/>

    <SeekBar
        android:id="@+id/form16_seekBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:max="10"/>

    <Switch
        android:id="@+id/form16_switch3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Did you have a recent injury?" />

    <Switch
        android:id="@+id/form16_switch4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Is your neck stiff?" />

    <Switch
        android:id="@+id/form16_switch5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Do you feel a tingling sensation?" />

    <Button
        android:id="@+id/form16_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ok" />

</androidx.appcompat.widget.LinearLayoutCompat>