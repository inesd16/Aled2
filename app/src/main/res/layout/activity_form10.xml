<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".form.form10">

    <SeekBar
        android:id="@+id/form10_seekBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:max="10"/>

    <Switch
        android:id="@+id/form10_switch2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/do_you_feel_like_your_ear_is_clogged_do_you_have_a_loss_in_hearing" />

    <Switch
        android:id="@+id/form10_switch3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/is_there_oozing_of_your_ear" />

    <Switch
        android:id="@+id/form10_switch4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/do_you_have_a_fever_38_c_100_4_f" />

    <Switch
        android:id="@+id/form10_switch5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/do_you_feel_dizzy" />

    <Button
        android:id="@+id/form10_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ok" />


</androidx.appcompat.widget.LinearLayoutCompat>