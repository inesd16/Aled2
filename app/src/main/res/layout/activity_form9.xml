<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".form.form9">

    <SeekBar
        android:id="@+id/form9_seekBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:max="10"/>

    <SeekBar
        android:id="@+id/form9_seekBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:max="10" />

    <Switch
        android:id="@+id/form9_switch3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/do_you_have_a_loss_of_sensitivity_in_the_zone" />

    <Switch
        android:id="@+id/form9_switch4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/are_you_having_unusual_gas" />

    <Switch
        android:id="@+id/form9_switch5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/do_you_have_diarrhea" />

    <Button
        android:id="@+id/form9_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ok" />


</androidx.appcompat.widget.LinearLayoutCompat>